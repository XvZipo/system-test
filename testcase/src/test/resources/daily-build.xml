<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Daily Build">
  <listeners>
    <listener class-name="stest.tron.wallet.common.client.utils.DailyBuildReport"></listener>
  </listeners>
  <test name="daily build"  parallel = "classes" thread-count = "6">
    <packages>
      <package name="stest.tron.wallet.dailybuild.*">
        <exclude name="stest.tron.wallet.dailybuild.multisign"></exclude>
        <exclude name="stest.tron.wallet.dailybuild.http"></exclude>
        <exclude name="stest.tron.wallet.dailybuild.freezeV2"></exclude>
        <exclude name="stest.tron.wallet.dailybuild.zentrc20token"></exclude>
        <exclude name="stest.tron.wallet.dailybuild.separateExecution"></exclude>
      </package>
    </packages>
  </test>
  <test name="http" parallel = "classes" thread-count = "1">
    <packages>
      <package name="stest.tron.wallet.dailybuild.http.*">
      </package>
      <package name="stest.tron.wallet.dailybuild.freezeV2.*">
      </package>
      <package name="stest.tron.wallet.dailybuild.zentrc20token.*">
      </package>
      <package name="stest.tron.wallet.dailybuild.separateExecution.*">
      </package>
    </packages>
  </test>
</suite>